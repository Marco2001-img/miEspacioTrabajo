package com.mx.Abstra;

public class Carro extends TrasporteGneral{

	protected String trasmision;
	protected boolean QuemaCocos=false;
	
	@Override
	public void Trasporte() {
		System.out.println("carro encendido");
		
	}
	
	public Carro() {
		
	}

	public Carro(String color, String noPlacas, String marca, int año, String trasmision, boolean quemaCocos) {
		super();
		this.trasmision = trasmision;
		QuemaCocos = quemaCocos;
		this.color=color;
		this.noPlacas=noPlacas;
		this.marca=marca;
	}

	public String getTrasmision() {
		return trasmision;
	}

	public void setTrasmision(String trasmision) {
		this.trasmision = trasmision;
	}

	public boolean isQuemaCocos() {
		return QuemaCocos;
	}

	public void setQuemaCocos(boolean quemaCocos) {
		QuemaCocos = quemaCocos;
	}

	@Override
	public String toString() {
		return "Carro [color=" + color + ", noPlacas=" + noPlacas + ", marca=" + marca + ", año=" + año
				+ ", trasmision=" + trasmision + ", QuemaCocos=" + QuemaCocos + "]";
	}
	
	
	
	
	

}
