package HasMap;

import java.util.HashMap;

import principal.IMetodos;

public class ImplementacionGenerica implements IMetodos{
	protected HashMap<Object, Object> hash = new HashMap<Object, Object>();

	@Override
	public void guardar(Object key, Object value) {
		hash.put(object key , object value);
		
	}

	@Override
	public void Editar(Object object) {
		hash.replace(object, object);
		
	}

	@Override
	public Object Buscar(Object object) {
		return hash.get(object);
	}

	@Override
	public void Mostrar() {
		System.out.println(hash);
		
	}

	@Override
	public void Eliminar(Object object) {
		hash.remove(object);
		
	}
	

}
