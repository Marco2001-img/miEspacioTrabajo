package poo;

public class Main {
    String nombre;

    Main(String nombre) {
        this.nombre = nombre;
    }

    @Override
    public String toString() {
        return "Perro: " + nombre;
    }

	public static void main(String[] args) {
        Object miPerro = new Object("Firulais"); // Aunque es tipo Object, es un Perro
        System.out.println(miPerro.toString());  // Perro: Firulais
        System.out.println(miPerro.getClass());  // class Perro


	}

}
