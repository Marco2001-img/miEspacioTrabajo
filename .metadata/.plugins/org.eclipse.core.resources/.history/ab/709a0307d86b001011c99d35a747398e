package com.mx.carros;

public class Carros {
	private String color;
	private String marca;
	private double peso;
	private int kilometraje;
	
	public Carros() {
		
	}
	
	public Carros(String especie,String color,double peso, int numeroPatas) {
		this.especie = especie;
		this.color = color;
		this.peso = peso;
		this.numeroPatas = numeroPatas;
	}
	
}
